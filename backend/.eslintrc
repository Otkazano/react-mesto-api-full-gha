{
	"extends": "airbnb-base",
	"rules": {
		"no-underscore-dangle": ["error", { "allow": ["_id", ".js"] }],
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-underscore-dangle": ["error",{"allow":["_id"]}]
	},
	"plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
	"resolve": {
    "extensions": [".js", ".jsx"]
	}
}